#: models.py:13
msgid "password"
msgstr "Пароль"

#: models.py:14
msgid "user"
msgstr "Пользователь"

#: models.py:15
msgid "created"
msgstr "Дата создания"

#: models.py:23
msgid "password history entry"
msgstr "Элемент истории паролей"

#: models.py:24
msgid "password history entries"
msgstr "Элементы истории паролей"

#: forms/__init__.py:26
msgid "The two password fields didn't match."
msgstr "Два поля с паролями не совпадают."

#: forms/__init__.py:27
msgid "The new password was used before. Please enter another one."
msgstr "Новый пароль был использован раннее. Пожалуйста, введите еще один."

#: forms/__init__.py:30
msgid "New password"
msgstr "Новый пароль"

#: forms/__init__.py:33
msgid "New password confirmation"
msgstr "New password confirmation"

#: forms/__init__.py:96
msgid "Your old password was entered incorrectly. Please enter it again."
msgstr "Ваш старый пароль введен неверно. Пожалуйста, введите еще раз"

#: forms/__init__.py:98
msgid "The old and the new password are too similar."
msgstr "Старый и новый пароль слишком похожи."

#: forms/__init__.py:99
msgid "The old and the new password are the same."
msgstr "Старый и новый пароль равны."

#: forms/__init__.py:101
msgid "Old password"
msgstr "Старый пароль"

#: forms/__init__.py:150
msgid ""
"That e-mail address doesn't have an associated user account. Are you sure "
"you've registered?"
msgstr ""
"Это адрес электронной почты не связан с пользователем. Вы уверены "
"что зарегистрировались?"

#: forms/__init__.py:152
msgid ""
"The user account associated with this e-mail address cannot reset the "
"password."
msgstr ""
"Пользователь с таким адресом электронной почты не может менять пароль."

#: forms/__init__.py:155
msgid "E-mail"
msgstr "E-mail"

#: forms/validators.py:152
msgid "The new password contains ambiguous bidirectional characters."
msgstr "Новый пароль содержит читаемые неоднозначно символы."

#: forms/validators.py:171
msgid "The new password is based on a common sequence of characters."
msgstr "Новый пароль содержит обычную последовательность символов."

#: forms/validators.py:189
#, python-format
msgid ""
"The new password contains consecutive characters. Only %(count)d consecutive "
"character is allowed."
msgid_plural ""
"The new password contains consecutive characters. Only %(count)d consecutive "
"characters are allowed."
msgstr[0] ""
"Новый пароль содержит последовательные символы. Только %(count)d последовательный "
"символ разрешен."
msgstr[1] ""
"Новый пароль содержит последовательные символы. Только %(count)d последовательных "
"символа разрешено."
msgstr[2] ""
"Новый пароль содержит последовательные символы. Только %(count)d последовательных "
"символов разрешено."

#: forms/validators.py:250
#, python-format
msgid "Please choose a different password, %s."
msgstr "Пожалуйста, выберите другое пароль, %s."

#: forms/validators.py:279
msgid "The new password is not varied enough."
msgstr "Новый пароль не достаточно различается."

#: forms/validators.py:339
msgid "The new password is based on a dictionary word."
msgstr "Новый пароль основан на слове из словаря."

#: forms/validators.py:374
msgid "The new password contains invalid unicode characters."
msgstr "Новый пароль содержит недопустимые символы юникода."

#: forms/validators.py:411
#, python-format
msgid "The new password must contain %d or more letter."
msgid_plural "The new password must contain %d or more letters."
msgstr[0] "Новый пароль должен содержать %d или больше букв."
msgstr[1] "Новый пароль должен содержать %d или больше букв."
msgstr[2] "Новый пароль должен содержать %d или больше букв."

#: forms/validators.py:430
msgid "The new password is similar to an email address."
msgstr "Новый пароль похож на адрес электронной почты."

#: forms/validators.py:466
#, python-format
msgid "The new password must contain %d or more number."
msgid_plural "The new password must contain %d or more numbers."
msgstr[0] "Новый пароль должен содержать %d или больше чисел."
msgstr[1] "Новый пароль должен содержать %d или больше чисел."
msgstr[2] "Новый пароль должен содержать %d или больше чисел."

#: forms/validators.py:519
#, python-format
msgid "The new password must contain %d or more symbol."
msgid_plural "The new password must contain %d or more symbols."
msgstr[0] "Новый пароль должен содержать %d или больше символов."
msgstr[1] "Новый пароль должен содержать %d или больше символов."
msgstr[2] "Новый пароль должен содержать %d или больше символов."
