#: models.py:13
msgid "password"
msgstr "contraseña"

#: models.py:14
msgid "user"
msgstr "usuario"

#: models.py:15
msgid "created"
msgstr "creado"

#: models.py:23
msgid "password history entry"
msgstr "entrada del historial de contraseña"

#: models.py:24
msgid "password history entries"
msgstr "entradas del historial de contraseñas"

#: forms/__init__.py:26
msgid "The two password fields didn't match."
msgstr "Las dos campos de contraseña no coinciden."

#: forms/__init__.py:27
msgid "The new password was used before. Please enter another one."
msgstr "La nueva contraseña fue usada anteriomente. Por favor, ingresá otra."

#: forms/__init__.py:30
msgid "New password"
msgstr "Nueva contraseña"

#: forms/__init__.py:33
msgid "New password confirmation"
msgstr "Nueva confirmación de contraseña"

#: forms/__init__.py:96
msgid "Your old password was entered incorrectly. Please enter it again."
msgstr "Tu antigua contraseña fue ingresada incorrectamente. Por favor, ingresala de nuevo."

#: forms/__init__.py:98
msgid "The old and the new password are too similar."
msgstr "La contraseña antigua y la actual son demasiado similares."

#: forms/__init__.py:99
msgid "The old and the new password are the same."
msgstr "La contraseña antigua y la actual son iguales."

#: forms/__init__.py:101
msgid "Old password"
msgstr "Contraseña antigua"

#: forms/__init__.py:150
msgid ""
"That e-mail address doesn't have an associated user account. Are you sure "
"you've registered?"
msgstr "Esa dirección de correo electrónico no tiene una cuenta de usuario asociada. Estás seguro de estar registrado?"

#: forms/__init__.py:152
msgid ""
"The user account associated with this e-mail address cannot reset the "
"password."
msgstr "La cuenta de usuario asociada con esa dirección de correo electrónico no puede resetear la contraseña."

#: forms/__init__.py:155
msgid "E-mail"
msgstr "Correo Electrónico"

#: forms/validators.py:152
msgid "The new password contains ambiguous bidirectional characters."
msgstr "La nueva contraseña contiene caracteres bidireccionales ambiguos."

#: forms/validators.py:171
msgid "The new password is based on a common sequence of characters."
msgstr "La nueva contraseña se basa en una secuencia común de caracteres."

#: forms/validators.py:189
#, python-format
msgid ""
"The new password contains consecutive characters. Only %(count)d consecutive "
"character is allowed."
msgid_plural ""
"The new password contains consecutive characters. Only %(count)d consecutive "
"characters are allowed."
msgstr[0] "La nueva contraseña contiene caracteres consecutivos. Sólo se permite %(count)d caracter consecutivo."
msgstr[1] "La nueva contraseña se basa en una palabra del diccionario. Sólo se permiten %(count)d caracteres consecutivos."

#: forms/validators.py:250
#, python-format
msgid "Please choose a different password, %s."
msgstr "Por favor, elegí una contraseña diferente, %s."

#: forms/validators.py:279
msgid "The new password is not varied enough."
msgstr "La nueva contraseña no es lo suficientemente variada."

#: forms/validators.py:339
msgid "The new password is based on a dictionary word."
msgstr "La nueva contraseña se basa en una palabra del diccionario."

#: forms/validators.py:374
msgid "The new password contains invalid unicode characters."
msgstr "La nueva contraseña contiene caracteres unicode inválidos."

#: forms/validators.py:411
#, python-format
msgid "The new password must contain %d or more letter."
msgid_plural "The new password must contain %d or more letters."
msgstr[0] "La nueva contraseña debe contener %d o más letras."
msgstr[1] "La nueva contraseña debe contener %d o más letras."

#: forms/validators.py:430
msgid "The new password is similar to an email address."
msgstr "La nueva contraseña es similar a una dirección de correo electrónico."

#: forms/validators.py:466
#, python-format
msgid "The new password must contain %d or more number."
msgid_plural "The new password must contain %d or more numbers."
msgstr[0] "La nueva contraseña debe contener %d o más números."
msgstr[1] "La nueva contraseña debe contener %d o más números."

#: forms/validators.py:519
#, python-format
msgid "The new password must contain %d or more symbol."
msgid_plural "The new password must contain %d or more symbols."
msgstr[0] "La nueva contraseña debe contener %d o más símbolos."
msgstr[1] "La nueva contraseña debe contener %d o más símbolos."
