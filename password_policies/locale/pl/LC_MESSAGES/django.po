#: models.py:13
msgid "password"
msgstr "hasło"

#: models.py:14
msgid "user"
msgstr "użytkownik"

#: models.py:15
msgid "created"
msgstr "utworzono"

#: models.py:23
msgid "password history entry"
msgstr "wpis w historii haseł"

#: models.py:24
msgid "password history entries"
msgstr "wpisy w historii haseł"

#: forms/__init__.py:26
msgid "The two password fields didn't match."
msgstr "Podane hasła nie są takie same."

#: forms/__init__.py:27
msgid "The new password was used before. Please enter another one."
msgstr "Nowe hasło było już użyte. Proszę wpisać inne."

#: forms/__init__.py:30
msgid "New password"
msgstr "Nowe hasło"

#: forms/__init__.py:33
msgid "New password confirmation"
msgstr "Potwierdzenie nowego hasła"

#: forms/__init__.py:96
msgid "Your old password was entered incorrectly. Please enter it again."
msgstr "Podano nieprawidłowe stare hasło. Proszę spróbować ponownie."

#: forms/__init__.py:98
msgid "The old and the new password are too similar."
msgstr "Stare i nowe hasło są zbyt podobne."

#: forms/__init__.py:99
msgid "The old and the new password are the same."
msgstr "Stare i nowe hasło jest takie samo."

#: forms/__init__.py:101
msgid "Old password"
msgstr "Stare hasło"

#: forms/__init__.py:150
msgid ""
"That e-mail address doesn't have an associated user account. Are you sure "
"you've registered?"
msgstr ""
"Podany adres e-mail nie ma przypisanego konta użytkownika. Upewnij się że "
"konto zostało utworzone."

#: forms/__init__.py:152
msgid ""
"The user account associated with this e-mail address cannot reset the "
"password."
msgstr ""
"Konto użytkownika przypisane do podanego adresu e-mail nie ma możliwości "
"resetowania hasła."

#: forms/__init__.py:155
msgid "E-mail"
msgstr "E-mail"

#: forms/validators.py:152
msgid "The new password contains ambiguous bidirectional characters."
msgstr "Nowe hasło zawiera niejednoznaczne dwukierunkowe znaki."

#: forms/validators.py:171
msgid "The new password is based on a common sequence of characters."
msgstr "Nowe hasło zawiera powszechnie znane ciągi znaków."

#: forms/validators.py:189
#, python-format
msgid ""
"The new password contains consecutive characters. Only %(count)d consecutive "
"character is allowed."
msgid_plural ""
"The new password contains consecutive characters. Only %(count)d consecutive "
"characters are allowed."
msgstr[0] ""
"Nowe hasło zawiera następujące po sobie znaki. Tylko %(count)d znak jest "
"dozwolony."
msgstr[1] ""
"Nowe hasło zawiera następujące po sobie znaki. Tylko %(count)d znaki są "
"dozwolone."
msgstr[2] ""
"Nowe hasło zawiera następujące po sobie znaki. Tylko %(count)d znaków jest "
"dozwolonych."

#: forms/validators.py:250
#, python-format
msgid "Please choose a different password, %s."
msgstr "Proszę wybrać inne hasło, %s."

#: forms/validators.py:279
msgid "The new password is not varied enough."
msgstr "Nowe hasło nie jest wystarczająco zróżnicowane."

#: forms/validators.py:339
msgid "The new password is based on a dictionary word."
msgstr "Nowe hasło bazuje na wyrażeniu słownikowym."

#: forms/validators.py:374
msgid "The new password contains invalid unicode characters."
msgstr "Nowe hasło zawiera nieprawidłowe znaki unicode."

#: forms/validators.py:411
#, python-format
msgid "The new password must contain %d or more letter."
msgid_plural "The new password must contain %d or more letters."
msgstr[0] "Nowe hasło musi zawierać %d lub więcej liter."
msgstr[1] "Nowe hasło musi zawierać %d lub więcej liter."
msgstr[2] "Nowe hasło musi zawierać %d lub więcej liter."

#: forms/validators.py:430
msgid "The new password is similar to an email address."
msgstr "Nowe hasło jest podobne do adresu email."

#: forms/validators.py:466
#, python-format
msgid "The new password must contain %d or more number."
msgid_plural "The new password must contain %d or more numbers."
msgstr[0] "Nowe hasło musi zawierać %d lub więcej cyfr."
msgstr[1] "Nowe hasło musi zawierać %d lub więcej cyfr."
msgstr[2] "Nowe hasło musi zawierać %d lub więcej cyfr."

#: forms/validators.py:519
#, python-format
msgid "The new password must contain %d or more symbol."
msgid_plural "The new password must contain %d or more symbols."
msgstr[0] "Nowe hasło musi zawierać %d lub więcej symboli."
msgstr[1] "Nowe hasło musi zawierać %d lub więcej symboli."
msgstr[2] "Nowe hasło musi zawierać %d lub więcej symboli."
